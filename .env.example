AGENTORA_MAX_TEAM_TURNS=6
AGENTORA_MAX_HANDOFFS=8
AGENTORA_ENABLE_SINGLE_AGENT_FALLBACK=true
AGENTORA_FORCE_SYNTHESIS_ON_BUDGET_EXHAUST=true
AGENTORA_ENABLE_DESKTOP_ACTIONS=true
AGENTORA_ENABLE_BROWSER_ACTIONS=true
AGENTORA_ACTION_REQUIRE_APPROVAL_DEFAULT=ask_once
AGENTORA_ALLOWED_PATH_ROOTS=.
AGENTORA_BLOCKED_PATH_ROOTS=
AGENTORA_ALLOWED_DOMAINS=localhost,127.0.0.1
AGENTORA_BLOCKED_DOMAINS=
AGENTORA_ALLOWED_APPS=
AGENTORA_MAX_ACTION_STEPS=12
AGENTORA_MAX_WORKFLOW_DURATION_SECONDS=300
AGENTORA_OPERATOR_MAX_STEP_RETRIES=3
AGENTORA_OPERATOR_DEFAULT_MODE=stepwise

# Optional advanced controls
AGENTORA_PARALLEL_LIMIT=4
