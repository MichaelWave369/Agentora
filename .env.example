AGENTORA_CONTEXT_LAYER_BUDGETS={"L0_HOT":2,"L1_SHORT":3,"L2_SESSION":2,"L3_DURABLE":2,"L4_SPARSE":2}
AGENTORA_MEMORY_DECAY_SHORT=0.85
AGENTORA_MEMORY_DECAY_MEDIUM=0.95
AGENTORA_MEMORY_DECAY_LONG=0.995
AGENTORA_MEMORY_LAYER_WEIGHTS={"L0_HOT":1.25,"L1_SHORT":1.0,"L2_SESSION":0.92,"L3_DURABLE":0.88,"L4_SPARSE":0.8,"L5_COLD":0.35}
